{
	"name": "iizuna",
	"version": "3.3.0",
	"description": "A TypeScript Framework for non-pwa pages (like content management systems)",
	"keywords": [
		"typescript",
		"non-pwa",
		"framework",
		"decorators"
	],
	"main": "lib/main.js",
	"types": "lib/main.d.ts",
	"scripts": {
		"build": "tsc",
		"prepare": "npm run build",
		"test": "nyc mocha -r ts-node/register -r jsdom-global/register tests/unit/**/*.spec.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/iizunats/iizuna.git"
	},
	"nyc": {
		"cache": true,
		"extension": [
			".ts"
		],
		"exclude": [
			"lib",
			"coverage",
			"**/*.d.ts"
		],
		"reporter": [
			"html"
		]
	},
	"author": "Tim RÃ¼cker",
	"contributors": [],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/iizunats/iizuna/issues"
	},
	"homepage": "http://iizunats.com",
	"engines": {},
	"dependencies": {
		"es6-promise": "^4.2.6",
		"typed-rest-client": "^1.1.2"
	},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^5.2.6",
		"chai": "^4.2.0",
		"jsdom": "^13.2.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^5.2.0",
		"nyc": "^13.3.0",
		"ts-node": "^7.0.1",
		"typedoc": "^0.8.0",
		"typedoc-plugin-internal-external": "^1.0.10",
		"typescript": "^3.3.3333"
	}
}
